<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<Pane prefHeight="100.0" prefWidth="70.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Button fx:id="card" mnemonicParsing="false" prefHeight="100.0" prefWidth="70.0">
         <graphic>
            <AnchorPane prefHeight="100.0" prefWidth="70.0">
               <children>
                  <ImageView fx:id="img" fitHeight="55.0" fitWidth="57.0" layoutX="1.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@com/avatarduel/card/image/land/Northern%20Water%20Tribe.png" />
                     </image></ImageView>
               </children>
            </AnchorPane>
         </graphic>
      </Button>
   </children>
</Pane>
